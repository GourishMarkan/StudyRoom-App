{
  "expo": {
    "name": "studyroom",
    "slug": "studyroom",
    "version": "1.0.5",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0077B6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.harsh10021.studyroom"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppIcon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 8,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.harsh10021.studyroom"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Splash.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-asset",
        {
          "assets": "**/*"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f952f89-d1f7-4778-ad5e-62961d75b1d4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4f952f89-d1f7-4778-ad5e-62961d75b1d4"
    }
  }
}
